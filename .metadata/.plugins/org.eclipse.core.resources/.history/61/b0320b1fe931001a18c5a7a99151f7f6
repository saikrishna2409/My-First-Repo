package javaapp;
class A1 
{
	public A1()
	{
		super();
		System.out.println("1");
	}
	public  A1(int a)
	{
		this();
		System.out.println("2");
	}
	{
		System.out.println("3");
	}
}
class B1 extends A1
{
	public B1()
	{
		super(10);
		System.out.println("4");
	}
	
	{
		System.out.println("5");
	}
}

public class Arr {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A1 a =new A1();
		A1 c=new A1(10);
		B1 b =new B1();

	}

}
