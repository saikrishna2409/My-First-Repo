package javaapp;

public class LambdaExpressions {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Hello hello =new Hello() {
			@Override
			public void display() {
				System.out.println("This is Inner Class");
			}
			
		};
		hello.display();
	}

}
interface Hello
{
	public void display();
}
