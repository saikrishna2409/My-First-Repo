package javaapp;

public class StringBuff {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		StringBuffer sb =new StringBuffer("Hello");
		System.out.println(sb.capacity());
		sb.append("Thoraipakkam");
		System.out.println(sb.capacity());
		sb.append(" Chennai");
		System.out.println(sb.capacity());
		sb.append(" Kurnool");
		System.out.println(sb.capacity());
	}

}
